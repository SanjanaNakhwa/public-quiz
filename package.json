{
  "name": "public-quiz-device",
  "version": "6.4.4",
  "description": "Access TV Quiz PhD",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "action": "./action/index",
    "build": "tsc && pnpm p",
    "lint": "eslint src docs",
    "lint:src": "eslint src",
    "lint:docs": "eslint docs",
    "develop": "ts-node-esm src/index.ts",
    "p": "pkg -t node18-linux-x64 --no-bytecode --public-packages \"*\" --public --out-dir . dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tvquizphd/public-quiz-device.git"
  },
  "author": "tvquizphd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tvquizphd/public-quiz-device/issues"
  },
  "homepage": "https://github.com/tvquizphd/public-quiz-device#readme",
  "dependencies": {
    "@octokit/app": "^13.0.11",
    "@octokit/request": "^6.2.2",
    "argon2": "^0.29.1",
    "eccrypto": "^1.1.6",
    "jws": "^4.0.0",
    "libsodium-wrappers": "0.7.8",
    "octokit": "^2.0.7",
    "opaque-low-io": "^3.2.1",
    "sock-secret": "4.3.0",
    "ts-node": "^10.9.1"
  },
  "pnpm": {
    "overrides": {
      "node-abi": "3.25.0",
      "libsodium-wrappers-sumo": "0.7.8"
    }
  },
  "devDependencies": {
    "@types/eccrypto": "^1.1.3",
    "@types/jws": "^3.2.4",
    "@types/libsodium-wrappers": "^0.7.9",
    "@types/node": "^18.7.19",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.24.0",
    "pkg": "^5.8.0",
    "typescript": "^4.8.3"
  }
}
